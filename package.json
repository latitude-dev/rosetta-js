{
  "name": "rosetta-ai",
  "version": "1.3.0",
  "author": "Latitude Data SL",
  "license": "MIT",
  "description": "The translation layer for LLM provider messages",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript={\"sourceMap\":false}",
    "dev": "rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript={\"sourceMap\":false} --watch",
    "test": "vitest run",
    "lint": "biome lint . && biome check . && tsc --noEmit",
    "format": "biome format --write . && biome lint --write . && biome check --write ."
  },
  "keywords": [
    "llm",
    "ai",
    "openai",
    "anthropic",
    "google",
    "vercel-ai",
    "translation",
    "messages",
    "genai"
  ],
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions",
    "node >= 20"
  ],
  "dependencies": {
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.0.0",
    "@types/node": "^22.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "tslib": "^2.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.0"
  }
}
